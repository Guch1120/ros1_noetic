[global_config]
  enabled_plugins = 
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[child2]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
[profiles]
  [[default]]
    use_system_font = False
    font = Monospace 12
[window_state]
  geometry_hint = False
EOF
